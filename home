interface User {
  firstName: string;
}
 
function getUsername( user: User ): string {
  // ... lots of code here
  return user.firstName;
}
 
const myRegularUser = { firstName: 'john', theme: 'blue' };
const myAdminUser = { firstName: 'bob', dbKey: 12345, theme: 'green', accessLevel: 5 };
getUsername( myRegularUser );

